{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "mongodb2"
  },
  "spec": {
    "selector": {
      "role": "mongo",
      "environment": "stest"
    },
    "ports": [
      {
        "name": "http",
        "protocol": "TCP",
        "port": 27017,
        "targetPort": 27017
      }
    ],
    "type": "ClusterIP"
  }
}
